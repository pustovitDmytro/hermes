    "eslint-plugin-sonarjs": "^0.16.0",
    "eslint-plugin-unicorn": "^36.0.0",
    "fs-extra": "^10.1.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.2",
    "jscpd": "^3.5.1",
    "json-query": "^2.2.2",
    "lockfile-lint": "^4.9.6",
    "mocha": "^10.1.0",
    "mocha-junit-reporter": "^2.2.0",
    "nyc": "^15.1.0",
