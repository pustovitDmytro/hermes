{
  "name": "hermes",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:sample": "http-server tests/sample -p 3001",
    "start:build": "http-server build -p 3002",
    "start:firefox": "web-ext run --source-dir build/firefox --verbose --start-url http://localhost:3001",
    "build:firefox": "webpack --config webpack.config.js && web-ext build --source-dir build/firefox --verbose --artifacts-dir build -o",
    "firefox": "concurrently --kill-others \"webpack --config webpack.config.js --watch\" \"npm run start:firefox\" \"npm run start:sample\"",
    "prepare": "husky install",
    "coveralls": "cat ./reports/coverage/lcov.info | coveralls",
    "lint:js": "eslint src bin tests --no-error-on-unmatched-pattern",
    "lint:firefox": "web-ext lint --source-dir build/firefox --verbose",
    "test:security": "npm audit --production && npm run test:lock",
    "test:mocha": "mocha --config .mocharc.json 'tests/**/*test.js'",
    "test:lint": "npm run lint:js && npm run lint:firefox",
    "test:package": "npt test -c .package-tester.json",
    "test:coverage": "nyc mocha --config .mocharc.json 'tests/**/*test.js'",
    "test:debt": "jscpd src --config .jscpd.json",
    "test:lock": "lockfile-lint --path package-lock.json --validate-https --allowed-hosts npm",
    "coverage-check": "MODE=test nyc check-coverage",
    "test": "npm run test:lint && npm run test:security && npm run test:mocha",
    "semantic-release": "npx semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pustovitDmytro/hermes.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pustovitDmytro/hermes/issues"
  },
  "homepage": "https://github.com/pustovitDmytro/hermes#readme",
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/node": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.20.1",
    "@google/semantic-release-replace-plugin": "^1.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "babel-loader": "^8.3.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.7",
    "cls-hooked": "^4.2.2",
    "concurrently": "^7.5.0",
    "conventional-changelog-eslint": "^3.0.9",
    "copy-webpack-plugin": "^11.0.0",
    "coveralls": "^3.1.1",
    "css-loader": "^6.7.2",
    "danger": "^11.2.0",
    "docopt": "^0.6.2",
    "eslint": "^7.32.0",
    "eslint-config-incredible": "^2.4.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-censor": "^1.5.2",
    "eslint-plugin-import": "^2.26.0",
